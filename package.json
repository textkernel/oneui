{
    "name": "@textkernel/oneui",
    "oneui": {
        "libraryName": "oneui"
    },
    "version": "11.0.0",
    "description": "Library of reusable React components with theming support",
    "main": "dist/oneui.min.js",
    "types": "dist/dts/src/index.d.ts",
    "scripts": {
        "storybook": "start-storybook -s ./stories/static -p 9001 -c .storybook",
        "storybook:build": "build-storybook -s ./stories/static -c .storybook -o .out",
        "storybook:deploy": "storybook-to-ghpages --script=storybook:build --out=.out",
        "build": "webpack --config ./scripts/build/webpack.dev.config.js",
        "build:prod": "webpack --config ./scripts/build/webpack.prod.config.js",
        "lint": "npm run lint:ci -- --fix",
        "lint:ci": "eslint . --ext .js,.ts,.tsx",
        "lint:styles": "stylelint 'src/**/*.s(a|c)ss'",
        "tsc": "tsc --noEmit --skipLibCheck",
        "test": "jest --config scripts/test/jest.config.js",
        "test:coverage": "jest --config scripts/test/jest.config.js --coverage",
        "check-npm-publish-access": "sh scripts/check-npm-publish-access/check-npm-publish-access.sh",
        "prepublishOnly": "npm-run-all --parallel check-npm-publish-access tsc test lint:* build:prod",
        "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
        "release": "standard-version -t ''"
    },
    "files": [
        "/dist"
    ],
    "standard-version": {
        "scripts": {
            "posttag": "git push --follow-tags origin master && npm publish --access=public"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm-run-all --parallel tsc test lint:ci lint:styles"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/textkernel/oneui.git"
    },
    "author": "Textkernel B.V. <open-source@textkernel.nl>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/textkernel/oneui/issues"
    },
    "homepage": "https://github.com/textkernel/oneui#readme",
    "sideEffects": false,
    "dependencies": {
        "@popperjs/core": "~2.1.1",
        "@react-google-maps/api": "~1.8.6",
        "@textkernel/bem": "~0.2.4",
        "@tippy.js/react": "~3.1.1",
        "css-vars-ponyfill": "~2.2.1",
        "downshift": "~5.2.7",
        "fast-memoize": "~2.5.2",
        "prop-types": "~15.7.2",
        "randomcolor": "~0.5.4",
        "rc-slider": "~9.2.0",
        "react-modal": "~3.11.2",
        "react-virtualized": "~9.21.2",
        "remove-accents": "~0.4.2"
    },
    "peerDependencies": {
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-icons": "^3.7.0",
        "react-transition-group": "^4.3.0"
    },
    "devDependencies": {
        "@babel/cli": "7.8.4",
        "@babel/core": "7.9.0",
        "@babel/plugin-proposal-class-properties": "7.8.3",
        "@babel/plugin-proposal-decorators": "7.8.3",
        "@babel/preset-env": "7.9.0",
        "@babel/preset-react": "7.9.4",
        "@sambego/storybook-state": "2.0.1",
        "@storybook/addon-actions": "5.3.18",
        "@storybook/addon-console": "1.2.1",
        "@storybook/addon-info": "5.3.18",
        "@storybook/addon-knobs": "5.3.18",
        "@storybook/react": "5.3.18",
        "@storybook/storybook-deployer": "2.8.6",
        "@types/jest": "24.9.1",
        "@types/node": "12.12.34",
        "@types/react": "16.9.31",
        "@types/react-virtualized": "9.21.8",
        "@typescript-eslint/eslint-plugin": "2.26.0",
        "@typescript-eslint/parser": "2.26.0",
        "autoprefixer": "9.8.0",
        "babel-jest": "24.9.0",
        "babel-loader": "8.1.0",
        "clean-webpack-plugin": "3.0.0",
        "core-js": "2.6.11",
        "coveralls": "3.1.0",
        "css-loader": "3.4.2",
        "enzyme": "3.11.0",
        "enzyme-adapter-react-16": "1.15.2",
        "enzyme-to-json": "3.4.4",
        "eslint": "6.8.0",
        "eslint-config-airbnb-typescript": "7.2.0",
        "eslint-config-prettier": "6.10.1",
        "eslint-loader": "3.0.4",
        "eslint-plugin-import": "2.20.2",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "eslint-plugin-prettier": "3.1.2",
        "eslint-plugin-react": "7.19.0",
        "eslint-plugin-react-hooks": "3.0.0",
        "file-loader": "6.0.0",
        "husky": "4.2.3",
        "jest": "24.9.0",
        "jest-css-modules-transform": "4.0.0",
        "mini-css-extract-plugin": "0.9.0",
        "node-sass": "4.13.1",
        "npm-run-all": "4.1.5",
        "optimize-css-assets-webpack-plugin": "5.0.3",
        "postcss-import": "12.0.1",
        "postcss-loader": "3.0.0",
        "prettier": "2.0.2",
        "react": "16.13.1",
        "react-docgen-typescript-loader": "3.7.2",
        "react-dom": "16.13.1",
        "react-icons": "3.10.0",
        "react-router-dom": "5.1.2",
        "react-transition-group": "4.3.0",
        "regenerator-runtime": "0.13.5",
        "sass-loader": "8.0.2",
        "standard-version": "7.1.0",
        "style-loader": "1.2.1",
        "stylelint": "13.5.0",
        "stylelint-config-recommended": "3.0.0",
        "stylelint-config-recommended-scss": "4.2.0",
        "stylelint-order": "4.0.0",
        "stylelint-scss": "3.17.2",
        "stylelint-webpack-plugin": "1.2.3",
        "ts-jest": "24.3.0",
        "ts-loader": "6.2.2",
        "typescript": "3.8.3",
        "webpack": "4.42.1",
        "webpack-bundle-analyzer": "3.6.1",
        "webpack-cli": "3.3.11",
        "webpack-dev-server": "3.10.3"
    }
}
